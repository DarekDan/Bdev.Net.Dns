<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <targets>
    <target xsi:type="Console" name="console" layout="${longdate} ${level} ${message} ${exception:format=tostring}" />
    <target xsi:type="Trace" name="trace" layout="${longdate} ${level} ${message} ${exception:format=tostring}" />
  </targets>
  <rules>
    <logger name="*" minlevel="Debug" writeTo="console,trace" />
  </rules>
</nlog>